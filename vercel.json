{
    "version": 2,
    "functions": {
        "api/app.py": {
            "runtime": "python3.14"
        }
    },
    "files": [
        "static/**",
        "templates/**"
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}