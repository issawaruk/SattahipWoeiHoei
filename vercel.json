{
    "version": 2,
    "functions": {
        "api/app.py": {
            "runtime": "python3.12"
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/api/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}